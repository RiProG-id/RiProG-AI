echo "4096,87380,404480" > "/proc/sys/net/ipv4/tcp_rmem"
echo "westwood" > "/proc/sys/net/ipv4/tcp_congestion_control
)
[[ -f /data/adb/modules/RiProG/feature/tweak/fstrim ]] && (
